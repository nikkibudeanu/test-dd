{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es6",
    "outDir": "build",
    "module": "commonjs",
    "moduleResolution": "node",
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": ["./src/doordash/doordash-react"]
}